openapi: 3.0.3
info:
  title: Memtest
  description: This is the API definition for the memtest-app
  contact:
    email: witzlch88229@th-nuernberg.de
  license:
    name: Apache 2.0
    url: http://www.apache.org/licenses/LICENSE-2.0.html
  version: 1.0.11
servers:
  - url: http://localhost/api/v1/
tags:
  - name: Test-Result
    externalDocs:
      description: Find out more
      url: http://swagger.io
paths:
  /testresult/upload/{qrcode-uuid}:
    put:
      tags:
        - Test-Result
      summary: "Updates a test result zip file"
      parameters:
        - in: path
          name: qrcode-uuid
          required: true
          schema:
            type: string
          description: "UUID associated with the QR code"
      responses:
        '200':
          description: "Successful operation"
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ApiResponse'
      requestBody:
        required: true
        description: "Encrypted ZIP file containing updated test results"
        content:
          application/zip:
            schema:
              type: string
              format: binary
              description: "Encrypted ZIP file"

components:
  schemas:
    ApiResponse:
      type: object
      properties:
        code:
          type: integer
          format: int32
        type:
          type: string
        message:
          type: string
      xml:
        name: ApiResponse
